xterm*faceName: Fira Mono:size=10
!xterm*faceName: DejaVu Sans Mono:size=10
xterm*font: 7x13
XTerm*foreground: rgb:FF/FF/FF
XTerm*background: rgb:00/00/00
XTerm*locale: true
XTerm*selectToClipboard: true
XTerm*termName: xterm-256color

URxvt*font: xft:Fira Mono:size=10, xft:DejaVu Sans Mono:size=10, xft:IPAGothic:size=14

URxvt.keysym.C-0: command:\033]710;xft:Fira Mono:size=10, xft:DejaVu Sans Mono:size=10, xft:IPAGothic:size=14\007
URxvt.keysym.C-minus: command:\033]710;xft:Fira Mono:size=5, xft:DejaVu Sans Mono:size=5, xft:IPAGothic:size=7\007
URxvt.keysym.C-equal: command:\033]710;xft:Fira Mono:size=30, xft:DejaVu Sans Mono:size=30, xft:IPAGothic:size=42\007

!URxvt*font: 7x13
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/e300
URxvt*foreground: rgb:FF/FF/FF
!URxvt*background: rgb:00/00/00
URxvt*color4: rgb:00/37/da
URxvt*color12: rgb:5c/5c/ff
URxvt*color8: rgba:8000/8000/8000/f000
URxvt*cursorColor2: rgb:00/00/00
URxvt*scrollBar: false 

! Fake transparencies
!URxvt*inheritPixmap: true
!URxvt*transparent: true
! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*shading: 5

st.font: Fira Mono:pixelsize=15:antialias=true:autohint:true;
st.opacity: 227
st.foreground: #ffffff
st.background: #000000

Xft.dpi: 120
